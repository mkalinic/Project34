<!-- -----------------------------------   lets rock -->
<div id="content" ng-controller="Project_controller" data-ng-init="init()">
    <div class="container-fluid" id="banner-main" style = "background-image:url({{GetProjectImageAddress(TopImage)}})"  >
 
        <div class="row">
            <div class="col-md-6 col-sm-8 col-xs-10 col-md-offset-3 col-sm-offset-2 col-xs-offset-1">
                <div class="banner-well well">
                    <div class="title">{{'Admin Page' | translate}}</div>
                    <div class="projecttitle">   {{Project.name}}</div>
                </div>
            </div>
        </div>
    </div>
    <!-- ------- general  -->
    <div class="container tenders">
        <div class="row tender-forms">
            <div class="row" style="text-align:center">
                <h3 >{{'general' | translate}}</h3>
            </div>
            <form class="form-horizontal">
                <div class="col-md-5 col-md-offset-1 col-sm-offset-1 col-sm-5 col-xs-12">
                    <div class="form-group">
                        <div class="col-sm-12">
                            <label for="Projectnaam">{{'Project name' | translate}}</label>
                            <input type="text" class="form-control" id="Projectnaam" ng-model="Project.name">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-12">
                            <label for="Opdrachtgever">{{'Client' | translate}}</label>
                            <input type="text" class="form-control" id="Opdrachtgever" ng-model="Project.Client.companyName">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-12">
                            <label for="Fase">{{'Phase' | translate}}</label>
                            <select class="form-control" id="Sexe" ng-model="Project.curentPhase" ng-options="x.ID as x.name for x in Project.Phases"></select>
</div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-12">
                            <label for="Omschrijving">{{'Description' | translate}}</label>
                            <textarea class="form-control" rows="3" id="Omschrijving"  ng-model="Project.description"></textarea>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <div class="col-sm-6">
                            <div class="img-upload">
                                <img ng-src="{{GetProjectImageAddress(Image)}}" width="280" />
                            </div>
                        </div>
                        <div class="col-sm-6">
                         
                            <button class="btn btn-block" id="Wijzig afbeelding" data-toggle="modal" data-target="#Upload" ngf-select="uploadImage($files)"    ngf-multiple="true">{{'Change image' | translate}} </button>
 
                             <!-- ADD IMAGE CHANGE HERE  -->
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-6">
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox" class="check-form" ng-click="digitalFilingChanged($event)"> {{'digital filing' | translate}}
                                </label>
                            </div>
                            <div id="datepicker">
                                <label>{{'timeCompleted' | translate}}</label>
                                <!------- datepicker --->
                                <div class="datepicker_novi">
                                    <div class="input-group"datepicker-popup="{{ datePickerGeneral.format}}" ng-model="Project.timeCompleted" is-open="datePickerGeneral.opened" min="datePickerGeneral.minDate" max="'2018-06-22'" datepicker-options="datePickerGeneral.dateOptions" date-disabled="datePickerGeneral.disabled(date, mode)" ng-required="true" close-text="Close" show-weeks="false" />
                                    <input type="text" value="{{ShowTime(Project.timeCompleted)}}" class="form-control" />    
                                    <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar" ng-click="datePickerGeneral.open($event)"></span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox" class="check-form" ng-model="Project.onFrontPage"  >{{'Show on homepage' | translate}}
                                </label>
                            </div>
                            <label id="status"> Status: </label><span >{{Project.status}} </span>
                        </div>
                        <div class="col-sm-6">
                            <button class="btn btn-block" type="button" data-toggle="modal" data-target="#kluisopenen" ng-click="OpenVault()" ng-show="OpenVaultShow">{{'opening vault' | translate}}</button>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-6 col-sm-offset-6">
                            <button class="btn btn-block" ng-click="Save()">{{'Save' | translate}}</button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div class="container tenders" ng-show="ShowDetails">
        <div class="row">
            <h3>{{'Schedule' | translate}}</h3>
        </div>
        <div class="row tender-forms">
            <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12">
                <div class="table-responsive">
                    <table class="table table-condensed table-striped new-project">
                        <tbody>
                            <!-- ----------------- Milestones ----------- -->
                            <tr ng-repeat="milestone in Project.Milestones">
                                <td class="first-row">{{'Start procurement' | translate}}</td>
                                <td>{{milestone.name}}</td>
                                <td>{{milestone.date}}</td>
                                <td>{{milestone.visibleFor}}</td>
                                <td>{{ShowTime(milestone.time)}}</td>
                                <td class="last-row remove">
                                    <button class="btn-remove" ng-click="removeMilestone(milestone)">{{'Remove' | translate}}</button>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <form class="form-horizontal">
                <div class="col-md-4 col-md-offset-2 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <div class="col-sm-12">
                            <label for="Bestandsnaam">{{'name' | translate}}</label>
                            <input type="text" class="form-control" id="Bestandsnaam" ng-model="NewMilestone.name">
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <div class="col-sm-12">
                            <div id="datepicker1">
                                <label>{{'Date' | translate}} </label>
                                <!------- datepicker --->
                                <div class="datepicker_novi">
                                    <!------- <pre>Selected date is: <em>{{dt | date:'fullDate' }}</em></pre> --->
                                    <div class="input-group">
                                        <input type="text" class="form-control" datepicker-popup="{{datePickerSchedule.format}}" ng-model="NewMilestone.time" is-open="datePickerSchedule.opened" min="datePickerSchedule.minDate" max="'2018-06-22'" datepicker-options="datePickerSchedule.dateOptions" date-disabled="datePickerSchedule.disabled(date, mode)" ng-required="true" close-text="Close" show-weeks="false" />
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar" ng-click="open($event)"></span>
                                        </span>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <div class="col-md-8 col-md-offset-2 col-sm-6 col-xs-12">
                    <div class="form-group">
                    <div class="col-sm-12">
                    <label for="Zichtbaar voor">{{'visible to' | translate}}</label>
                    </div>
                            <div class="inline">
                            <div class="col-sm-6">
                            <select ng-model="NewMilestone.visibleFor" ng-options="x for x in rootScope_userTypes"></select>
                            </div>
                            <!--<select class="form-control" id="Zichtbaar voor">
                                <option>Advieurs</option>
                                <option>Advieurs</option>
                                <option>Advieurs</option>
                                <option>Advieurs</option>
                            </select>-->
                            <div class="col-sm-6">
                             <button class="btn btn-block" ng-click="addMilestone(NewMilestone)">{{'Add' | translate}}</button>
                             </div>
                             </div>
                        </div>
                    
                </div>
                
            </form>
        </div>
    </div>
    
    <!-- ----------------- TextBlocks ----------- -->
    <div class="container tenders" ng-repeat="textBlock in Project.TextBlocks"  ng-show="ShowDetails">
     <div class="row" style="text-align:center;">
        <h3>{{'text boxes' | translate}} </h3>
    </div>
        <div class="row tender-forms">
            <form class="form-horizontal"  >
                <div class="col-md-4 col-md-offset-2 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <div class="col-sm-12">
                            <label for="Omschrijving1">{{'Description' | translate}}</label>
                            <input type="text" class="form-control" id="Omschrijving1" ng-model="textBlock.text">
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <div class="col-sm-12">
                            <div id="datepicker2">
                                <label>{{'Date' | translate}}</label>
                                <div class="datepicker_novi">
                                    <div class="input-group" datepicker-popup="dd-MM-yyyy" ng-model="textBlock.time" is-open="datePickers[$index].opened" datepicker-options="datePickers[$index].dateOptions" date-disabled="datePickers[$index].disabled(date, mode)" ng-required="true" close-text="Close" show-weeks="false" ng-click="datePickers[$index].open($event)" >
                                        <input type="text" value="{{ShowTime(textBlock.time)}}" class="form-control"/>
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar" ng-click="datePickers[$index].open($event)"></span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12">
                    <div class="table-responsive">
                        <table class="table table-condensed table-striped new-project files">
                            <tbody>
                                <!-- ----------------- TextBlock Files ----------- -->
                                <tr ng-repeat="file in textBlock.Files">
                                    <td class="first-row">{{file.file}}</td>
                                    <td>{{file.displayName}}</td>
                                    <td>{{toMegabytes(file.size)}} MB </td>
                                    <td class="last-row remove">
                                        <button class="btn-remove" ng-click="removeFile(file)">{{'remove' | translate}}</button>
                                    </td>
                                </tr>
 
                            </tbody>
                        </table>
                    </div>
                </div>
               
                <div class="col-md-4 col-md-offset-2 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <div class="col-sm-12">
                            <label for="Bsetandsnaam">{{'filename' | translate}}</label>
                            <input type="text" class="form-control" id="Bsetandsnaam" ng-model="NewTextBlockFile.displayName">
                        </div>
                    </div>
